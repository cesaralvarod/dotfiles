	async function findAllOrdersToReport(params) {
	  try {
	    let orders = await orderModel
	      .find(params)
	      .select(
	        'code createdDate name lastName email phone address paymentMethod paymentInfo',
	      )
	      .sort({ createdDate: -1 });
	    return orders;
	  } catch (error) {
	    return error.message;
	  }
	}
