	const { Schema, model } = require('mongoose');
	
	const couponSchema = new Schema(
	  {
	    idStore: {
	      type: Schema.Types.ObjectId,
	      required: true,
	    },
	    createdBy: {
	      types: Schema.Types.ObjectId,
	    },
	    modifiedBy: {
	      types: Schema.Types.ObjectId,
	    },
	    code: {
	      type: String,
	      required: true,
	    },
	    type: {
	      type: String,
	      enum: ['percentage', 'fixed_value'],
	      required: true,
	    },
	    value: {
	      type: Number,
	      required: true,
	    },
	    start_date: {
	      type: Date,
	      default: Date.now(),
	    },
	    expiration_date: {
	      type: Date,
	      required: true,
	    },
	    stock: {
	      type: Number,
	    },
	  },
	  { timestamps: true },
	);
	
	module.exports = model('coupon', couponSchema);
